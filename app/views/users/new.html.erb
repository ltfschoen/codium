<div class="page-content">
  <h2>Create User</h2>


  <%= form_for(@user, :url => {:action => 'createstudent'}) do |f| %>
  <div class="form-group">
    <div><%= f.label :email %><br />
      <%= f.email_field :email, :autofocus => true, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.label :password %><br />
      <%= f.password_field :password, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.label :password_confirmation %><br />
      <%= f.password_field :password_confirmation, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.label :given_name %><br />
      <%= f.text_field :given_name, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.label :family_name %><br />
      <%= f.text_field :family_name, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.label :username %><br />
      <%= f.text_field :username, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.label :tagline %><br />
      <%= f.text_field :tagline, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.label :bio %><br />
      <%= f.text_area :bio, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.labl :linkedin %><br />
      <%= f.text_field :linkedin, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
  <div><%= f.labl :twitter %><br />
      <%= f.text_field :twitter, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div><%= f.labl :github %><br />
      <%= f.text_field :github, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
  <div><%= f.labl :website %><br />
      <%= f.text_field :website, :class => "form-control" %>
    </div>
  </div>
  <div class="form-group">
    <div>
      <%= f.label :image %> <br/>
      <%= f.file_field :image %>
    </div>
  </div>

  <div><%= f.submit "Create User", :class => "btn btn-primary" %></div>
  <% end %>

</div>
